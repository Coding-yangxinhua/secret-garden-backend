<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pwc.sdc.sg.mapper.SubscribeMapper">

    <resultMap id="BaseResultMap" type="com.pwc.sdc.sg.model.Subscribe">
            <id property="id" column="ID" jdbcType="BIGINT"/>
            <result property="name" column="NAME" jdbcType="VARCHAR"/>
            <result property="price" column="PRICE" jdbcType="DECIMAL"/>
            <result property="ratio" column="RATIO" jdbcType="INTEGER"/>
            <result property="validTime" column="VALID_TIME" jdbcType="INTEGER"/>
            <result property="validUses" column="VALID_USES" jdbcType="INTEGER"/>
            <result property="gmtCreate" column="GMT_CREATE" jdbcType="TIMESTAMP"/>
            <result property="gmtModified" column="GMT_MODIFIED" jdbcType="TIMESTAMP"/>
            <result property="deleted" column="DELETED" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID,NAME,PRICE,
        RATIO,VALID_TIME,VALID_USES,
        GMT_CREATE,GMT_MODIFIED,DELETED
    </sql>
</mapper>
